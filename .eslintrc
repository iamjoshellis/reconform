{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "prettier/prettier": 2,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "react/sort-comp": 0
  }
}